<body>
    <div class="report-header">
        <div class="report-header-container">
            <h1 class="report-title">S.A.C. - Reporte Basico de Obras</h1>
        </div>
        <div class="report-details">
            <div class="detail-item">
                <label>Grupo:</label>&nbsp;<p id="txtGrupo"></p>
            </div>
            <div class="detail-item">
                <label>Estado Contratos:</label>&nbsp;<p id="txtEstadoContrato"></p>
            </div>
            <div class="detail-item">
                <label>Tipo Contrato:</label>&nbsp;<p id="txtTipoContrato"></p>
            </div>
            <div class="detail-item">
                <label>Región:</label>&nbsp;<p id="txtRegion"></p>
            </div>
            <div class="detail-item">
                <label>Unidad/Depto:</label>&nbsp;<p id="txtClase"></p>
            </div>
            <div class="detail-item">
                <label>Fecha incial:</label>&nbsp;<p id="txtFechaInicial"></p>
            </div>
            <div class="detail-item">
                <label>Fecha final:</label>&nbsp;<p id="txtFechaFinal"></p>
            </div>
        </div>
    </div>
    <div id="mensaje"></div>
    <div id="table-container" class="hidden">
        <div class="col-lg-12">
            <div class="">
                <table id="tablaReporteBasicoObras" class="table table-bordered table-striped nowrap" cellspacing="0" style="width:100%; font-size:12px">
                    <thead>
                        <tr>
                            <th>Estado Contrato</th>
                            <th>Región</th>
                            <th class="">Carpeta</th>
                            <th class="">SAFI</th>
                            <th class="">Centro Gestión</th>
                            <th class="">Administración</th>
                            <th class="">Nombre</th>
                            <th class="">Resolución</th>
                            <th class="">Fecha Resolución</th>
                            <th class="">Fecha Trámite</th>
                            <th class="">Fecha A. Económica</th>
                            <th class="">Fecha A. Técnica</th>
                            <th class="">Contratista</th>
                            <th class="">Residente</th>
                            <th class="">Inspector Fiscal</th>
                            <th class="">Visitador</th>
                            <th class="">Asesoría</th>
                            <th class="">Financiamiento</th>
                            <th class="">Licitación</th>
                            <th class="">Tipo Reajuste</th>
                            <th class="">Índice Base</th>
                            <th class="">Presupuesto Oficial</th>
                            <th class="">Presupuesto Inicial</th>
                            <th class="">Monto Mod. Tramitadas</th>
                            <th class="">Monto Actual (ma)</th>
                            <th class="">Pagado (pa)</th>
                            <th class="">Monto Reajuste</th>
                            <th class="">Fecha Inicio</th>
                            <th class="">Plazo Original</th>
                            <th class="">Fecha Termino Original</th>
                            <th class="">Mod. Días Tramitados</th>
                            <th class="">Fecha Termino Legal</th>
                            <th class="">Fecha Termino Real</th>
                            <th class="">Mod. Días Pre-Aprob.</th>
                            <th class="">Mod. Monto Pre-Aprob.</th>
                            <th class="">Mod. Días No-Aprob.</th>
                            <th class="">Mod. Monto No-Aprob.</th>
                            <th class="">Longitud</th>
                            <th class="">Carpeta</th>
                            <th class="">Tipo Contrato</th>
                            <th class="">Cód. Mercado Público</th>
                        </tr>
                    </thead>
                    <tbody id="tbl_body_table"></tbody>
                </table>
            </div>
        </div>
    </div>
    <!-- Scripts -->
    <script src="~/Vendors/jquery/jquery.min.js"></script>
    <script src="~/Vendors/bootstrap/dist/js/bootstrap.bundle.min.js"></script>

    <!-- DataTables -->
    <script type="text/javascript" charset="utf-8" src="~/Vendors/datatable/jquery.dataTables.min.js"></script>

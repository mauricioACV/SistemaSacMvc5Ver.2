                        <label>Inspector Fiscal</label>
                        <input type="text" class="form-control input-radius" placeholder="Click para seleccionar" name="InspectorFiscal" id="txtInspectorFiscal" data-toggle="modal" data-target="#modalInspectorFiscal" readonly="readonly" />

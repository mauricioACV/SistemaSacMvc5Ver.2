}
@section styles{
    <!-- agregar estilos -->
    <link href="~/Content/EstilosSac/Spinner.css" rel="stylesheet" />
    <!-- agregar estilos -->

    <style>
        .pointer {
            cursor: pointer;
        }

        .containerBtnComuna {
